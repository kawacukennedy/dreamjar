    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.3",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.5.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0",
    "jest": "^29.6.1",
    "@types/jest": "^29.5.3",
    "eslint": "^8.47.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1"
  }
